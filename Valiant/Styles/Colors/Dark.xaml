<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="WindowPrimaryColor">#140032</Color>
    <Color x:Key="WindowSecondaryColor">Black</Color>

    <Color x:Key="AccentPrimaryColor">#9762F5</Color>
    <Color x:Key="AccentSecondaryColor">#714AB7</Color>

    <Color x:Key="ControlLightestColor">#1A000000</Color>
    <Color x:Key="ControlLighterColor">#32000000</Color>
    <Color x:Key="ControlLightColor">#4C000000</Color>
    <Color x:Key="ControlMediumColor">#66000000</Color>
    <Color x:Key="ControlDarkColor">#7E000000</Color>
    <Color x:Key="ControlDarkerColor">#99000000</Color>
    <Color x:Key="ControlDarkestColor">#B3000000</Color>

    <Color x:Key="BorderColor">#1AFFFFFF</Color>
    <Color x:Key="BorderLightColor">#27FFFFFF</Color>
    <Color x:Key="BorderLighterColor">#34FFFFFF</Color>

    <Color x:Key="TextLighterColor">#FFFFFFFF</Color>
    <Color x:Key="TextLightColor">#C0FFFFFF</Color>
    <Color x:Key="TextMediumColor">#81FFFFFF</Color>
    <Color x:Key="TextDarkColor">#3FFFFFFF</Color>
    <Color x:Key="TextDarkerColor">#25FFFFFF</Color>
</ResourceDictionary>